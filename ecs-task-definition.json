{
    "family": "factorial-app",
    "executionRoleArn": "arn:aws:iam::271867616940:role/ecsadmin",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "factorial-app",
            "image": "271867616940.dkr.ecr.us-east-2.amazonaws.com/factorial-app:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
